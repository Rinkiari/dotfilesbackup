{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 20,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.accessibilityPageSize": 12,
  "explorer.confirmDelete": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.singleQuote": true,
  "prettier.semi": true,
  "prettier.trailingComma": "all",
  "prettier.bracketSameLine": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 100,
  "editor.formatOnSave": true,
  "volar.format.initialIndent": {
    "html": true
  },
  "explorer.confirmPasteNative": false,
  "editor.minimap.size": "fit",
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.fontFamily": "IBMPlexMono, Menlo, Monaco, 'Courier New', monospace",
  "animations.Install-Method": "Apc Customize UI++",
  "apc.imports": [
    "file://${userHome}/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.4/dist/updateHandler.js"
  ],
  "workbench.colorTheme": "Andromeda",
  "terminal.integrated.fontSize": 16,
  "window.zoomPerWindow": false,
  "window.zoomLevel": 0.4,
  "editor.renderLineHighlight": "none",
  "editor.cursorStyle": "underline",
  "editor.find.cursorMoveOnType": false,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.lineHeight": 1.6,
  "explorer.compactFolders": false,
  "breadcrumbs.enabled": false,
  "terminal.integrated.cursorWidth": 0,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorStyleInactive": "block"
}
